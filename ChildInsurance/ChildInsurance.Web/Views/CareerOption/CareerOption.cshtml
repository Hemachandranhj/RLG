@model ChildInsurance.Web.Models.CareerOptionViewModel

@{
    Layout = null;
    ViewBag.Title = "CareerOption";
}

<!DOCTYPE HTML>

<html>
<head>
    <link type="text/css" rel="stylesheet" href="../../bundles/css/materialize.min.css" />
    <link type="text/css" rel="stylesheet" href="../../bundles/css/custom.css" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body bg-color="#202a34">
    <header>
        <div class="container">
            <a href="#" data-activates="nav-mobile" class="button-collapse top-nav full hide-on-large-only">
                <i class="material-icons">menu</i>
            </a>
        </div>
        <div class="row top-nav nomargin">
            <div class="col l12 s12 m12">
                <ul id="dropdown1" class="dropdown-content">
                    <li>
                        <a href="#!">one</a>
                    </li>
                    <li>
                        <a href="#!">two</a>
                    </li>
                    <li class="divider"></li>
                    <li>
                        <a href="#!">three</a>
                    </li>
                </ul>
                <nav>
                    <div class="nav-wrapper">
                        <ul class="right">
                            <li class="username">
                                Welcome
                                <span class="title">Shine Taylor</span>
                                <img src="../../bundles/images/yuna.jpg" alt="" class="circle">
                            </li>
                            <li>
                                <a class="dropdown-button" href="#!" data-activates="dropdown1">
                                    <i class="material-icons">mail_outline</i>
                                </a>
                            </li>
                            <li>
                                <a href="badges.html">
                                    <i class="material-icons">notifications</i>
                                </a>
                            </li>
                            <li>
                                <a href="collapsible.html">Logout</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <div class="row skew-blue nomargin">
            <div class="col l1 s1 m1">
                <h5>City</h5>
            </div>
            <div class="col l2 s2 m2">
                <select class="browser-default">
                    <option value="" disabled selected>Choose your option</option>
                    <option value="1">Chennai</option>
                    <option value="2">Coimbatore</option>
                </select>
            </div>
            <div class="col l1 s1 m1">
                <h5>School</h5>
            </div>
            <div class="col l2 s2 m2">
                <select class="browser-default">
                    <option value="" disabled selected>Choose your option</option>
                    <option value="1">School 1</option>
                    <option value="2">School 2</option>
                    <option value="3">School 3</option>
                </select>
            </div>
        </div>
        </div>
    </header>
    <aside>
        <div id="nav-mobile" class="side-nav fixed blue-grey darken-4 ">
            <div class="row nomargin">
                <div class="col l11 collection nomargin">
                    <div class="collection-item avatar blue-grey darken-4 white-text">
                        <img src="../../bundles/images/yuna.jpg" alt="" class="circle">
                        <p class="title">Shine Taylor</p>
                        <p class="nomargin">
                            <span class="online">&#8226;</span>
                            Online
                        </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <nav class="col l10 offset-l1 custom-search">
                    <div class="nav-wrapper">
                        <form>
                            <div class="input-field">
                                <input id="search" type="search" required>
                                <label for="search">
                                    <i class="material-icons">search</i>
                                </label>
                            </div>
                        </form>
                    </div>
                </nav>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col s6 l6 m6 dark-blue cards">
                        <span class="cards1"></span>
                        <p>Track Progress</p>
                    </div>
                    <div class="col s6 l6 m6 skew-blue cards">
                        <span class="cards2"></span>
                        <p>Event Planning</p>
                    </div>
                    <div class="col s6 l6 m6 pure-blue cards">
                        <span class="cards3"></span>
                        <p>Track Events</p>
                    </div>
                    <div class="col s6 l6 m6 grey-blue cards">
                        <span class="cards4"></span>
                        <p>Track Review Meetings</p>
                    </div>
                    <div class="col s12 l12 m12 skew-blue cards">
                        <span class="cards5"></span>
                        <p>Connect with Export Groups</p>
                    </div>
                    <div class="col s6 l6 m6 grey-blue cards">
                        <span class="cards6"></span>
                        <p>Crowd Funding</p>
                    </div>
                    <div class="col s6 l6 m6 pure-blue cards">
                        <span class="cards7"></span>
                        <p>Manage School Insurance</p>
                    </div>
                    <div class="col s6 l6 m6 dark-blue cards">
                        <span class="cards8"></span>
                        <p>Financial Recommedation</p>
                    </div>
                    <div class="col s6 l6 m6 skew-blue cards">
                        <span class="cards9"></span>
                        <p>Career Recommedation</p>
                    </div>
                </div>
            </div>
        </div>
    </aside>
    <main>
        <br>
        <br>
        <div class="row white-text">
            <h3 class="col offset-l1 offset-s1 offset-m1">Academics</h3>
        </div>
        <div class="row white-text">
            <div class="col l5 s5 m5 offset-l1 offset-s1 offset-m1">
                <div style="float:top;position: relative;top: 1px;left: -10px;font-size: 34px;background-color: #1a2226;width: 220px;">Interests</div>
            </div>
            <div class="col l5 s5 m5  " align="right">
                <div style="position: relative;top: 0px;right: -10px;font-size: 34px;width: 250px;background-color: #1a2226;" align="right">Career Option</div>
            </div>
            
            
            <div class="col l5 s5 m5  offset-l1 offset-s1 offset-m1 factfind" style="height: 220px;">
                <div class="row">
                    
                    
                    <div class="col l12 s12 m12 offset-l1 offset-m1 offset-s1">something</div>
                
                </div>
            </div>
            
            
            <div class="col l5 s5 m5  factfind" style="height: 220px;">
                <div class="row">
                    <div class="col l12 s12 m12 offset-l1 offset-m1 offset-s1">something</div>
                </div>
            </div>
        </div>

        <br />

        <div class="row white-text">
            <h3 class="col offset-l1 offset-s1 offset-m1">Extra Curricular</h3>
        </div>
        <div class="row white-text">
            <div class="col l5 s5 m5 offset-l1 offset-s1 offset-m1">
                <div style="float:top;position: relative;top: 1px;left: -10px;font-size: 34px;background-color: #1a2226;width: 220px;">Interests</div>
            </div>
            <div class="col l5 s5 m5  " align="right">
                <div style="position: relative;top: 0px;right: -10px;font-size: 34px;width: 250px;background-color: #1a2226;" align="right">Career Option</div>
            </div>
            <div class="col l5 s5 m5  offset-l1 offset-s1 offset-m1 factfind" style="height: 220px;">
                <div class="row">
                    @if (Model != null)
                    {
                        foreach (var model in Model.NonAcademicCareerRecommedation)
                        {
                            foreach (var interest in model.Interests)
                            {
                                <div class="col l12 s12 m12 offset-l1 offset-m1 offset-s1">@interest.InterestName</div>
                            }
                        }
                    }
                    
                </div>
            </div>
            <div class="col l5 s5 m5  factfind" style="height: 220px;">
                <div class="row">
                    <div class="col l12 s12 m12 offset-l1 offset-m1 offset-s1">something</div>
                </div>
            </div>
        </div>


    </main>
    <script src="../../bundles/js/jquery-2.1.4.min.js"></script>
    <script type="text/javascript">
        $('.button-collapse').sideNav();

        function draw() {
            var canvas = document.getElementById('canvas');
            if (canvas.getContext) {
                var ctx = canvas.getContext('2d');

                ctx.beginPath();
                ctx.moveTo(75, 50);
                ctx.lineTo(100, 75);
                ctx.lineTo(100, 25);
                ctx.fill();
            }
        }
    </script>
</body>
</html>
