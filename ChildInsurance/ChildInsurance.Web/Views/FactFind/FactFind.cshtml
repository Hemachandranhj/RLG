@model ChildInsurance.Web.Models.FactFindViewModel
@{
    Layout = null;
    ViewBag.Title = "FactFind";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>index3</title>
    <link type="text/css" rel="stylesheet" href="../../bundles/css/materialize.min.css" />
    <link type="text/css" rel="stylesheet" href="../../bundles/css/custom.css" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body>
    <div class="container">
        <a href="#" data-activates="nav-mobile" class="button-collapse top-nav full hide-on-large-only">
            <i class="material-icons">menu</i>
        </a>
    </div>
    <div class="row top-nav nomargin">
        <div class="col l12 s12 m12">
            <ul id="dropdown1" class="dropdown-content">
                <li>
                    <a href="#!">one</a>
                </li>
                <li>
                    <a href="#!">two</a>
                </li>
                <li class="divider"></li>
                <li>
                    <a href="#!">three</a>
                </li>
            </ul>
            <nav>
                <div class="nav-wrapper">
                    <ul class="right">
                        <li class="username">
                            Welcome
                            <span class="title">Shine Taylor</span>
                            <img src="../../bundles/images/yuna.jpg" alt="" class="circle">
                        </li>
                        <li>
                            <a class="dropdown-button" href="#!" data-activates="dropdown1">
                                <i class="material-icons">mail_outline</i>
                            </a>
                        </li>
                        <li>
                            <a href="badges.html">
                                <i class="material-icons">notifications</i>
                            </a>
                        </li>
                        <li>
                            <a href="collapsible.html">Logout</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </div>
    <div class="row skew-blue nomargin">
        <div class="col l1 s1 m1">
            <h5>City</h5>
        </div>
        <div class="col l2 s2 m2">
            <select class="browser-default">
                <option value="1">Chennai</option>
                <option value="2">Coimbatore</option>
            </select>
        </div>
        <div class="col l1 s1 m1">
            <h5>School</h5>
        </div>
        <div class="col l2 s2 m2">
            <select class="browser-default">
                <option value="1">Vels Vidyashram</option>
                <option value="2">School 2</option>
                <option value="3">School 3</option>
            </select>
        </div>
    </div>
    <aside>
        <div id="nav-mobile" class="side-nav fixed blue-grey darken-4 ">
            <div class="row nomargin">
                <div class="col l11 collection nomargin">
                    <div class="collection-item avatar blue-grey darken-4 white-text">
                        <img src="../../bundles/images/yuna.jpg" alt="" class="circle">
                        <p class="title">Shine Taylor</p>
                        <p class="nomargin">
                            <span class="online">&#8226;</span>
                            Online



                        </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <nav class="col l10 offset-l1 custom-search">
                    <div class="nav-wrapper">
                        <form>
                            <div class="input-field">
                                <input id="search" type="search" required>
                                <label for="search">
                                    <i class="material-icons">search</i>
                                </label>
                            </div>
                        </form>
                    </div>
                </nav>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col s6 l6 m6 skew-blue cards">
                        <span class="cards9"></span>
                        <p>Career Recommedation</p>
                    </div>
                    <div class="col s6 l6 m6 dark-blue cards">
                        <span class="cards8"></span>
                        <p>Financial Recommedation</p>
                    </div>

                    <div class="col s6 l6 m6 dark-blue cards">
                        <span class="cards1"></span>
                        <p>Track Progress</p>
                    </div>
                    <div class="col s6 l6 m6 skew-blue cards">
                        <span class="cards2"></span>
                        <p>Event Planning</p>
                    </div>
                    <div class="col s6 l6 m6 pure-blue cards">
                        <span class="cards3"></span>
                        <p>Track Events</p>
                    </div>
                    <div class="col s6 l6 m6 grey-blue cards">
                        <span class="cards4"></span>
                        <p>Track Review Meetings</p>
                    </div>
                    <div class="col s12 l12 m12 skew-blue cards">
                        <span class="cards5"></span>
                        <p>Connect with Export Groups</p>
                    </div>
                    <div class="col s6 l6 m6 grey-blue cards">
                        <span class="cards6"></span>
                        <p>Crowd Funding</p>
                    </div>
                    <div class="col s6 l6 m6 pure-blue cards">
                        <span class="cards7"></span>
                        <p>Manage School Insurance</p>
                    </div>
                </div>
            </div>
        </div>
    </aside>
    <main>
        <br>
        <br>
        <br>
        <br>
        <br>
        <div class="row">


            <div class="col s9 m9 l9 offset-l1">
                <div class="white-text" style="background-color: #1a2226;
    /*position: absolute;*/
    /*float: left;*/
    font-size: 35px;
    width: 338px;
    bottom: 50%;
    padding-left: 17px;">Find Fact</div>
                @using (Html.BeginForm("FactFind", "FactFind", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                {
                <div class="card blue-grey darken-1">
                    <div class="card-content white-text factfind">
                        <div class="row">
                            <form class="col s12">
                                <div class="row">
                                    <div class="input-field col l3 offset-l2">
                                        Asset
                                    </div>
                                    <div class="input-field col l6">
                                        @Html.TextBoxFor(m => m.Asset, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col l3 offset-l2" align="left">
                                        Liability
                                    </div>
                                    <div class="input-field col s6">
                                        @Html.TextBoxFor(m => m.Liablity, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col l3 offset-l2" align="left">
                                        Income
                                    </div>
                                    <div class="input-field col s6">
                                        @Html.TextBoxFor(m => m.Income, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col l3 offset-l2" align="left">
                                        Expenditure
                                    </div>
                                    <div class="input-field col s6">
                                        @Html.TextBoxFor(m => m.Expenditure, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="row">
                                    @Html.HiddenFor(m => m.StudentId);
                                    <div class="input-field col l4 offset-l7">
                                        <input id="first_name" value="Recommeded plan" type="submit" class="validate waves-effect waves-light btn factfindbtn">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                }
            </div>
        </div>
    </main>
</body>
</html>
