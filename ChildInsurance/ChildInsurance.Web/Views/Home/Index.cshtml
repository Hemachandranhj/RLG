
@{
    Layout = null;
    ViewBag.Title = "Index";
}

<!DOCTYPE HTML>
<html>
<head>
    <link type="text/css" rel="stylesheet" href="../../bundles/css/materialize.min.css" />
    <link type="text/css" rel="stylesheet" href="../../bundles/css/custom.css" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body>
    <header>
        <div class="container">
            <a href="#" data-activates="nav-mobile" class="button-collapse top-nav full hide-on-large-only">
                <i class="material-icons">menu</i>
            </a>
        </div>
        <div class="row top-nav nomargin">
            <div class="col l12 s12 m12">
                <ul id="dropdown1" class="dropdown-content">
                    <li>
                        <a href="#!">one</a>
                    </li>
                    <li>
                        <a href="#!">two</a>
                    </li>
                    <li class="divider"></li>
                    <li>
                        <a href="#!">three</a>
                    </li>
                </ul>
                <nav>
                    <div class="nav-wrapper">
                        <ul class="right">
                            <li class="username">
                                Welcome
                                <span class="title">Shine Taylor</span>
                                <img src="../../bundles/images/yuna.jpg" alt="" class="circle">
                            </li>
                            <li>
                                <a class="dropdown-button" href="#!" data-activates="dropdown1">
                                    <i class="material-icons">mail_outline</i>
                                </a>
                            </li>
                            <li>
                                <a href="badges.html">
                                    <i class="material-icons">notifications</i>
                                </a>
                            </li>
                            <li>
                                <a href="collapsible.html">Logout</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <div class="row skew-blue nomargin">
            <div class="col l1 s1 m1">
                <h5>City</h5>
            </div>
            <div class="col l2 s2 m2">
                <select class="browser-default">
                    <option value="" disabled selected>Choose your option</option>
                    <option value="1">Chennai</option>
                    <option value="2">Coimbatore</option>
                </select>
            </div>
            <div class="col l1 s1 m1">
                <h5>School</h5>
            </div>
            <div class="col l2 s2 m2">
                <select class="browser-default">
                    <option value="" disabled selected>Choose your option</option>
                    <option value="1">School 1</option>
                    <option value="2">School 2</option>
                    <option value="3">School 3</option>
                </select>
            </div>
        </div>
    </header>
    <aside>
        <div id="nav-mobile" class="side-nav fixed blue-grey darken-4 ">
            <div class="row nomargin">
                <div class="col l11 collection nomargin">
                    <div class="collection-item avatar blue-grey darken-4 white-text">
                        <img src="../../bundles/images/yuna.jpg" alt="" class="circle">
                        <p class="title">Shine Taylor</p>
                        <p class="nomargin"><span class="online">&#8226;</span>Online</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <nav class="col l10 offset-l1 custom-search">
                    <div class="nav-wrapper">
                        <form>
                            <div class="input-field">
                                <input id="search" type="search" required><label for="search"><i class="material-icons">search</i></label>
                            </div>
                        </form>
                    </div>
                </nav>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col s6 l6 m6 skew-blue cards">
                        <span class="cards9"></span>
                        <a href="@Url.Action("Index", "CareerOption")"><p>Career Recommedation</p></a>
                    </div>
                    <div class="col s6 l6 m6 dark-blue cards">
                        <span class="cards8"></span>
                        <p>Financial Recommedation</p>
                    </div>
                    <div class="col s6 l6 m6 dark-blue cards">
                        <span class="cards1"></span>
                        <p>Track Progress</p>
                    </div>
                    <div class="col s6 l6 m6 skew-blue cards">
                        <span class="cards2"></span>
                        <p>Event Planning</p>
                    </div>
                    <div class="col s6 l6 m6 pure-blue cards">
                        <span class="cards3"></span>
                        <p>Track Events</p>
                    </div>
                    <div class="col s6 l6 m6 grey-blue cards">
                        <span class="cards4"></span>
                        <p>Track Review Meetings</p>
                    </div>

                    <div class="col s12 l12 m12 skew-blue cards">
                        <span class="cards5"></span>
                        <p>Connect with Export Groups</p>
                    </div>
                    <div class="col s6 l6 m6 grey-blue cards">
                        <span class="cards6"></span>
                        <p>Crowd Funding</p>
                    </div>

                    <div class="col s6 l6 m6 pure-blue cards">
                        <span class="cards7"></span>
                        <p>Manage School Insurance</p>
                    </div>
                </div>
            </div>
        </div>
    </aside>
    <main>
        <h4 class="head-content">Status of Events</h4>
        <div class="events">
            <div class="container">
                <div class="row">
                    <div class="col l12">
                        <div id="eventchart" class="chart"></div>
                    </div>
                </div>
            </div>
        </div>
        <h4 class="head-content">Event Scheduled for various Interest</h4>
        <div class="interest">
            <div class="container">
                <div class="row">
                    <div class="col l12">
                        <div id="interestchart" class="chart"></div>
                    </div>
                </div>
            </div>
        </div>
    </main>
    <script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
    <script src="../../bundles/js/materialize.min.js"></script>
    <script type="text/javascript">
        window.onload = function () {
            var data;
            var insurerChartData;
            var insurereventChartData;
            $.ajax({
                type: 'GET',
                url: "../../bundles/js/InsurerChartData.json",
                data: data,
                async: false,
                beforeSend: function (xhr) {
                    if (xhr && xhr.overrideMimeType) {
                        xhr.overrideMimeType('application/json;charset=utf-8');
                    }
                },
                dataType: 'json',
                success: function (data) {
                    insurerChartData = data;

                }
            });

            $.ajax({
                type: 'GET',
                url: "../../bundles/js/InsurerChartEventData.json",
                data: data,
                async: false,
                beforeSend: function (xhr) {
                    if (xhr && xhr.overrideMimeType) {
                        xhr.overrideMimeType('application/json;charset=utf-8');
                    }
                },
                dataType: 'json',
                success: function (data) {
                    insurereventChartData = data;

                }
            });


            var event = new CanvasJS.Chart("eventchart", {
                theme: "theme3",
                backgroundColor: "rgba(28, 44, 88, 1)",
                legend: {
                    maxWidth: 200,
                    itemWidth: 60
                },
                data: [{
                    type: "pie",
                    showInLegend: true,
                    legendText: "{indexLabel}",
                    dataPoints: insurereventChartData
                }]
            });
            event.render();
            var interest = new CanvasJS.Chart("interestchart", {
                theme: "theme3",
                backgroundColor: "rgba(28, 44, 88, 1)",
                legend: {
                    maxWidth: 200,
                    itemWidth: 60
                },
                data: [{
                    type: "pie",
                    showInLegend: true,
                    legendText: "{indexLabel}",
                    dataPoints: insurerChartData
                }]
            });
            interest.render();
        }
    </script>
    <script type="text/javascript" src="../../bundles/js/canvasjs.min.js"></script>
    <script type="text/javascript">
        $('.button-collapse').sideNav();
    </script>
</body>
</html>
